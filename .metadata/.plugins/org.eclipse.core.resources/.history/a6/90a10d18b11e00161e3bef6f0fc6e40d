package problem2;

public class problem2 {
	public static void main(String[] args) {
		long fib1 = 1;
		long fib2 = 2;
		long fib3 = 0;
		long total = 0;

		while (total < 100) {
			fib3 = fib1 + fib2;
		
			if (fib1 < fib2) {
				fib1 = fib3;
			} else {
				fib2 = fib3;
			}
			
			total = total + fib3;
			System.out.println(fib3);
			fib3 = 0;
		}

		
	}
}
