package problem2;

public class problem2 {
	public void main(String[] args) {
		int x = 4000000;
		long fib = 0;
		long total = 0;
		
		for (int i = 0; i < x; i++) {
			
			for (int a = 0; a <= i; a++) {
				
				fib += a; 
				
				if (fib % 2 == 0) {
					total += fib;
				}
				
				fib = 0;		
				
			}
			
		}
		
		System.out.println(total);
		
	}
}
